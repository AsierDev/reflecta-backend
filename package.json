{
  "name": "reflecta-backend",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:controllers": "jest src/controllers",
    "test:services": "jest src/services",
    "test:middlewares": "jest src/middlewares",
    "test:routes": "jest src/routes",
    "lint": "eslint --format stylish src/**/*.ts",
    "lint:fix": "eslint --format stylish src/**/*.ts --fix",
    "lint:strict": "eslint --format stylish src/**/*.ts --max-warnings 0",
    "lint:format": "prettier --check \"src/**/*.{ts,json}\"",
    "lint:format:fix": "prettier --write \"src/**/*.{ts,json}\"",
    "lint:all": "npm run lint:strict && npm run lint:format && npm run type-check",
    "lint:all:fix": "npm run lint:fix && npm run lint:format:fix",
    "type-check": "tsc --noEmit",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "clean": "rm -rf node_modules package-lock.json dist coverage",
    "clean:cache": "npm cache clean --force",
    "reinstall": "npm run clean && npm run clean:cache && npm install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.5.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "globals": "^13.24.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "winston": "^3.17.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.30",
    "@types/supertest": "^6.0.2",
    "@eslint/eslintrc": "^2.1.4",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "prisma": "^6.5.0",
    "supertest": "^7.1.0",
    "ts-jest": "^29.2.6",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.2"
  }
}
